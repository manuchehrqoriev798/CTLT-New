{
  "version": 2,
  "routes": [
    { "src": "/login", "dest": "/html/login.html" },
    { "src": "/register", "dest": "/html/register.html" },
    { "src": "/ctlt", "dest": "/html/ctlt.html" },
    { "src": "/moodle", "dest": "/html/moodle.html" },
    { "src": "/profile", "dest": "/html/profile.html" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" }
  ]
}